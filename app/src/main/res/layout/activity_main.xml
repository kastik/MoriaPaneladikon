<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/ypologismos_gel_buuton"
        android:layout_width="155dp"
        android:layout_height="82dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:text="@string/ypgel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.157" />

    <Button
        android:id="@+id/ypologismos_epal_button"
        android:layout_width="155dp"
        android:layout_height="82dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:text="@string/ypologismosepal"
        app:layout_constraintBottom_toBottomOf="@+id/ypologismos_gel_buuton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.424"
        app:layout_constraintStart_toEndOf="@+id/ypologismos_gel_buuton"
        app:layout_constraintTop_toTopOf="@+id/ypologismos_gel_buuton"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/anazitisi_thematon_button"
        android:layout_width="155dp"
        android:layout_height="82dp"
        android:text="@string/thema_search"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/ypologismos_gel_buuton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ypologismos_gel_buuton"
        app:layout_constraintTop_toBottomOf="@+id/ypologismos_gel_buuton"
        app:layout_constraintVertical_bias="0.18" />

    <Button
        android:id="@+id/anazitisi_baseon_button"
        android:layout_width="155dp"
        android:layout_height="82dp"
        android:text="@string/baseis"
        app:layout_constraintBottom_toBottomOf="@+id/anazitisi_thematon_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/anazitisi_thematon_button"
        app:layout_constraintTop_toTopOf="@+id/anazitisi_thematon_button" />

    <Button
        android:id="@+id/Launch_Gel_Upload"
        android:layout_width="155dp"
        android:layout_height="82dp"
        android:text="Upload Gel Activity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/anazitisi_thematon_button"
        app:layout_constraintStart_toStartOf="@+id/anazitisi_thematon_button"
        app:layout_constraintTop_toBottomOf="@+id/anazitisi_thematon_button"
        app:layout_constraintVertical_bias="0.18" />


</androidx.constraintlayout.widget.ConstraintLayout>